{
  "name": "@fujaba/fulib-yaml-ts",
  "version": "1.0.2",
  "description": "Typescript implementation of the fulib yamler",
  "author": "Sebastian Copei <seb@uni-kassel.de>",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/fujaba/fulibYamlTS"
  },
  "scripts": {
    "build": "tsc && tslint -c tslint.json -p tsconfig.json",
    "test": "jest"
  },
  "main": "./lib/src/index.js",
  "types": "./lib/src/index.d.ts",
  "dependencies": {
    "@types/node": "^11.9.5"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.0",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "moduleNameMapper": {
      "tests/(.*)": "<rootDir>/tests/$1"
    },
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public"
  }
}
